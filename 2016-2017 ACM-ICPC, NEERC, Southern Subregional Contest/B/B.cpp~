#include <bits/stdc++.h>
using namespace std;

pair<int,int> solve(int L,int R) {
	if (L == R) return make_pair(L ,L);
	int midl = (L + R) >> 1;
	int midr = midl + 1;
	pair<int,int> l = solve(L, midl), r = solve(midr, R);
	char A, B;
	
}
int main() {
	for (scanf("%d",&T); T; T--) {
		scanf("%d",&n);
		if (n == 1) {
			printf("! 1 1\n");
			fflush(stdout);
			continue;
		}
		ans = solve(1, n);
		printf("! %d %d\n",ans.first, ans.second);
		fflush(stdout);
	}
}
